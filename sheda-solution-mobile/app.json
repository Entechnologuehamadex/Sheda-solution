{
  "expo": {
    "name": "sheda-solution-mobile",
    "slug": "sheda-solution-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/sheda-splash.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.shedasolutionmobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/sheda-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.shedasolutionmobile",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/sheda-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/sheda-splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you post houses and profile photos"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1fb297b1-d2da-4ae9-a356-024ad093a2a7"
      }
    }
  }
}
